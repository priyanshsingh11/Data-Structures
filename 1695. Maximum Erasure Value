class Solution(object):
    def maximumUniqueSubarray(self, nums):
        left=0
        right=0
        mapping=set()
        total=0
        ans=0

        while (right<len(nums)):
            if (nums[right] not in mapping):
                mapping.add(nums[right])
                total+=nums[right]
                right+=1
                ans=max(ans,total)

            
            else:
                mapping.remove(nums[left])
                total-=nums[left]
                left+=1
            
        return ans
                
        
